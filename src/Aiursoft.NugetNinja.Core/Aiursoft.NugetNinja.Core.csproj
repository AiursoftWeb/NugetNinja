<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <OutputType>Library</OutputType>
        <TargetFramework>net7.0</TargetFramework>
        <AssemblyName>Aiursoft.NugetNinja.Core</AssemblyName>
        <RootNamespace>Aiursoft.NugetNinja.Core</RootNamespace>
        <IsTestProject>false</IsTestProject>
        <IsPackable>false</IsPackable>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <PackageId>Aiursoft.NugetNinja.Core</PackageId>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Aiursoft.Canon" Version="7.0.2" />
        <PackageReference Include="Aiursoft.CommandFramework" Version="7.0.1" />
        <PackageReference Include="Aiursoft.CSTools" Version="7.0.0" />
        <PackageReference Include="HtmlAgilityPack" Version="1.11.53" />
        <PackageReference Include="System.Linq.Async" Version="6.0.1" />
        <PackageReference Include="Microsoft.Extensions.Http" Version="7.0.0" />
    </ItemGroup>
</Project>